 {{#each results}}

<div class="article-container">
    <div class="title-container">
        <a href="https://news.google.com/{{ link }}" target="_blank"><h3>{{ title }}</h3></a>
    </div>
    <div class="save-button-container">
        <input type="button" class="button save-article" value="Save Article" article-id="{{ _id }}">
    </div>
</div>

{{/each}}